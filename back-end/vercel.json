{
  "name": "api-js",
  "version": 2,
  "public": true,
  "functions": {
    "index": {
      "handler": "dist/index.js",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
