{
  "name": "api-js",
  "version": 2,
  "public": true,
  "functions": {
    "dist/index.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "dist/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ]
}