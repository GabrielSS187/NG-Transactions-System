{
  "name": "api-js",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "dist/index.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ]
}
