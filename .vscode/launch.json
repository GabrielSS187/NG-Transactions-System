// .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Backend",
      "restart": true,
      "port": 9229,
      "cwd": "${workspaceFolder}/back-end",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": [
        "${workspaceFolder}/back-end/dist/**/*.js"
      ]
    }
  ]
}
